  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^8.2.5",
    "copy-webpack-plugin": "^11.0.0",
    "cypress": "^10.6.0",
    "html-webpack-plugin": "^5.5.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
